{
  "name": "my-blog-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm run barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "npm run barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "prepare": "is-ci || husky install"
  },
  "dependencies": {
    "@tsed/ajv": "^7.2.0",
    "@tsed/common": "^7.2.0",
    "@tsed/core": "^7.2.0",
    "@tsed/di": "^7.2.0",
    "@tsed/engines": "^7.2.0",
    "@tsed/exceptions": "^7.2.0",
    "@tsed/json-mapper": "^7.2.0",
    "@tsed/logger": "^6.3.2",
    "@tsed/logger-file": "^6.3.2",
    "@tsed/mongoose": "^7.2.0",
    "@tsed/platform-cache": "^7.2.0",
    "@tsed/platform-exceptions": "^7.2.0",
    "@tsed/platform-express": "^7.2.0",
    "@tsed/platform-log-middleware": "^7.2.0",
    "@tsed/platform-middlewares": "^7.2.0",
    "@tsed/platform-params": "^7.2.0",
    "@tsed/platform-response-filter": "^7.2.0",
    "@tsed/platform-views": "^7.2.0",
    "@tsed/schema": "^7.2.0",
    "@tsed/swagger": "^7.2.0",
    "ajv": "^8.11.0",
    "barrelsby": "^2.5.1",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "dotenv-expand": "^9.0.0",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "method-override": "^3.0.0",
    "mongoose": "^6.6.5"
  },
  "devDependencies": {
    "@tsed/cli-plugin-eslint": "4.1.0",
    "@tsed/cli-plugin-mongoose": "4.1.0",
    "@tsed/testing-mongoose": "^7.2.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/method-override": "^0.0.32",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.0",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "tslib": "^2.4.0",
